    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "eslint": "^8.11.0",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "firebase": "^9.6.9",
    "highcharts": "^10.0.0",
    "highcharts-react-official": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "package-lock.json": "^1.0.0",
    "prettier-eslint": "^13.0.0",
    "react": "^17.0.2",
    "react-customizable-progressbar": "^1.0.3",
    "react-dom": "^17.0.2",
    "react-elastic-carousel": "^0.11.5",
