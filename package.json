    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix": "yarn run prettier -- --write && yarn run eslint -- --fix",
    "lint": "eslint .",
    "prettier": "prettier-eslint $PWD/'**/*.{js,ts,tsx}' --write",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
